---
import { transitionEnabledOnThisPage } from 'astro:transitions/client';
import Title from '~/Atoms/Title.astro';
import { FONTS } from '~/constants/Fonts';

type Props = {
  class?: string;
  [key: string]: any;
};

const { class: className = '', ...props } = Astro.props as Props;
---

<aside
  {...props}
  class:list={className}
  class="flex h-screen w-full flex-row items-center justify-center bg-cover bg-center bg-no-repeat p-0"
>
  <slot />
</aside>
